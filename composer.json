{
    "name": "alexbarnsley/collection",
    "description": "A basic collection management package",
    "type": "library",
    "require": {
        "adbario/php-dot-notation": "^2.2",
        "php": "^7.0|^8.0"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "AlexBarnsley\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Barnsley",
            "email": "alex@barnsley.io"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit tests --coverage-text --coverage-html .coverage"
        ]
    }
}
